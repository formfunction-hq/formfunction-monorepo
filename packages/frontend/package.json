{
  "name": "formfn-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "delete-relay-codegen": "find . -name '*.graphql.ts' -type f -delete",
    "start": "craco start",
    "start-functions": "npx wrangler pages dev --kv nftImageByMint --kv nftNameByMint --kv userImageByUsername --kv userDisplayNameByUsername --kv seriesImageByUsernameSlashSlug --kv seriesNameByUsernameSlashSlug --kv launchDarkly --kv campaignInfo --kv nftAssetDimensionsByMint --binding API_URL=\"http://localhost:4000\" -- craco start",
    "build": "export REACT_APP_RELEASE=$CF_PAGES_COMMIT_SHA DISABLE_ESLINT_PLUGIN=true && craco build",
    "build-plain": "craco build",
    "postbuild": "export S_CLI=$(npm bin)/sentry-cli SENTRY_DISABLE_UPDATE_CHECK=true SENTRY_RELEASE=$CF_PAGES_COMMIT_SHA SENTRY_ORG=formfunction SENTRY_PROJECT=formfunction && $S_CLI releases new $SENTRY_RELEASE --finalize && $S_CLI releases deploys $SENTRY_RELEASE new -e $SENTRY_ENVIRONMENT && $S_CLI releases files $SENTRY_RELEASE upload-sourcemaps --ignore node_modules --validate ./ || true",
    "test": "jest \".*\\.test\\.ts\"",
    "test:coverage": "jest --coverage",
    "eject": "craco eject",
    "prettier": "prettier --check 'src/**/*.ts'",
    "eslint": "eslint --cache 'src/**/*.{ts,tsx}'",
    "lint": "yarn prettier --write && yarn eslint --fix",
    "gen-colors": "node scripts/genColors.js",
    "gen-fonts": "node scripts/genFonts.js",
    "gen-graphql": "rm *.graphql && npx -y get-graphql-schema http://localhost:8080/v1/graphql -h x-hasura-admin-secret=$HASURA_GRAPHQL_ADMIN_SECRET > schemas/hasura.graphql && npx -y get-graphql-schema http://localhost:8080/v1beta1/relay -h x-hasura-admin-secret=$HASURA_GRAPHQL_ADMIN_SECRET > schemas/relay.graphql && ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register scripts/genGraphqlSchema.ts",
    "relay": "yarn delete-relay-codegen && relay-compiler",
    "sandbox": "ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register src/scripts/sandbox.ts",
    "parseTx": "ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register src/scripts/parseTx.ts",
    "create-auction-house-testnet": "REACT_APP_SOLANA_NETWORK=testnet NODE_ENV=testnet ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register src/scripts/createAuctionHouse.ts",
    "create-auction-house-devnet": "NODE_ENV=development ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register src/scripts/createAuctionHouse.ts",
    "create-auction-house-mainnet": "NODE_ENV=production ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register src/scripts/createAuctionHouse.ts",
    "update-auction-house-devnet": "ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register src/scripts/updateAuctionHouse.ts",
    "update-auction-house-mainnet": "NODE_ENV=production REACT_APP_SOLANA_NETWORK=mainnet-beta ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register src/scripts/updateAuctionHouse.ts",
    "print-auction-house-devnet": "NODE_ENV=development REACT_APP_SOLANA_NETWORK=devnet ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register src/scripts/printAuctionHouse.ts",
    "print-auction-house-mainnet": "NODE_ENV=production REACT_APP_SOLANA_NETWORK=mainnet-beta ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register src/scripts/printAuctionHouse.ts",
    "withdraw-auction-house-devnet": "NODE_ENV=development REACT_APP_SOLANA_NETWORK=devnet ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register src/scripts/withdrawAuctionHouse.ts",
    "withdraw-auction-house-mainnet": "NODE_ENV=production REACT_APP_SOLANA_NETWORK=mainnet-beta ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register src/scripts/withdrawAuctionHouse.ts"
  },
  "dependencies": {
