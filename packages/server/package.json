{
  "name": "formfn-server",
  "version": "1.0.0",
  "description": "Express template",
  "main": "src/index.js",
  "repository": "https://github.com/arcticmatt/express-template.git",
  "author": "Matt Lim <mlimcollege@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "clean-prisma-schema": "gsed -i '/^\\/\\/\\/ The underlying table/d' prisma/schema.prisma",
    "prettier": "prettier --check 'src/**/*.ts'",
    "eslint": "eslint --cache 'src/**/*.ts'",
    "lint": "yarn prettier --write && yarn eslint --fix",
    "nodemon": "NODE_PATH=./dist nodemon --delay 2",
    "run-node": "NODE_PATH=./dist node",
    "tsc": "tsc",
    "ts-node": "ts-node -r dotenv/config -r tsconfig-paths/register --files",
    "gen-types": "DISABLE_LD=true ts-node --swc -r tsconfig-paths/register src/scripts/codegen.ts",
    "gen-prisma": "npx prisma db pull && npx prisma generate && yarn clean-prisma-schema",
    "gen-prisma-plain": "npx prisma generate",
    "get-main-graphql": "git show main:./src/__generated__/generated.graphql > ./src/__generated__/generated.main.graphql",
    "rm-main-graphql": "rm ./src/__generated__/generated.main.graphql",
    "check-graphql": "npx graphql-inspector diff ./src/__generated__/generated.main.graphql ./src/__generated__/generated.graphql",
    "sandbox": "DISABLE_LD=true ts-node -r tsconfig-paths/register src/scripts/sandbox.ts",
    "swc": "swc src -d dist/src",
    "test": "DISABLE_LD=true jest",
    "test:coverage": "jest --coverage",
    "hasura-console": "cd ./hasura-project && hasura console",
    "hasura-retry-prompt": "echo \"\nPlease retry after running export HASURA_GRAPHQL_ADMIN_SECRET=<secret from docker-compose-dev.yaml>\"",
    "hasura-migrate": "(cd ./hasura-project && hasura metadata apply --endpoint http://localhost:8080/ && hasura migrate apply --all-databases --endpoint http://localhost:8080/) || yarn hasura-retry-prompt",
    "hasura-reload": "(cd ./hasura-project && hasura metadata reload --endpoint http://localhost:8080/) || yarn hasura-retry-prompt",
    "process-finished-auctions": "ts-node -r tsconfig-paths/register src/scripts/processFinishedAuctionsScript.ts",
    "process-finished-pnft-drops": "ts-node -r tsconfig-paths/register src/scripts/processFinishedPnftDropsScript.ts",
    "remote-schema-reload": "curl -X POST http://localhost:8080/v1/metadata -H \"Content-Type: application/json\" -H \"X-Hasura-Role: admin\" -H \"X-Hasura-Admin-Secret: $HASURA_GRAPHQL_ADMIN_SECRET\" -d '{\"type\": \"reload_remote_schema\", \"args\": { \"name\": \"express-schema\" }}' | tee /dev/stderr | grep \"access-denied\" && yarn hasura-retry-prompt || exit 0",
    "updateAssetSizes": "ts-node-transpile-only -r tsconfig-paths/register src/scripts/updateAssetSizes.ts"
  },
